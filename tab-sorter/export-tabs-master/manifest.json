{
    "name": "Export Tabs",
    "version": "1.2",
    "description": "Export the URL of all opened tabs to text",
    "icons": {
        "16": "tabs_io.png",
        "48": "tabs_io_48.png",
        "128": "tabs_io_128.png"
    },
    "browser_action": {
        "default_icon": "tabs_io.png",
        "default_title": "Export Tabs",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "manifest_version": 2
}
